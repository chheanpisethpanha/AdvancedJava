<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <servlet>
        <servlet-name>InitServ</servlet-name>
        <servlet-class>InitServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SuccessServ</servlet-name>
        <servlet-class>SuccessServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FailServ</servlet-name>
        <servlet-class>FailServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServ</servlet-name>
        <servlet-class>LogoutServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InitServ</servlet-name>
        <url-pattern>/InitServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SuccessServ</servlet-name>
        <url-pattern>/SuccessServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FailServ</servlet-name>
        <url-pattern>/FailServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServ</servlet-name>
        <url-pattern>/LogoutServ</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
